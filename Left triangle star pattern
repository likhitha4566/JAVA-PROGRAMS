import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner sc = new Scanner(System.in);
	    int n =5;
	    for(int i=0;i<n;i++)
	    {
	      for(int j=0;j<n-i-1;j++)
	      {
	      System.out.print(" ");
	      }
	       for(int m=0;m<i+1;m++)
	      {
		   System.out.print("*");
	      }
	      System.out.println("");
	    }
	}	
}
